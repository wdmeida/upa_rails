
<div class="row">
  <h3 class="mx-auto">
    <%= @admins.model_name.human(count: @admins.length) %>
    <small class="text-muted"><%= t('labels.listed') %></small>
  </h3>    
</div>

<%= render partial: 'layouts/shared/search',
           locals: { 
                      path: backoffice_admins_path, 
                      field_name: :name_cont, 
                      type_button: 'outline-success' 
                  } %>

<%= link_to new_backoffice_admin_path, 
            :class => "btn btn-success btn-block btn-block-backoffice" do %>
  <%= t('buttons.new') %>
<% end %>

<div id="list-admins">
  <%= render partial: 'backoffice/admins/shared/admins' %>
</div>
